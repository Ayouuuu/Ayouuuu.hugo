    "qcloudcdn": "node .scripts/qcloudcdn.js $SECRET_ID $SECRET_KEY"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ayouuuu/Ayouuuu.hugo.git"
  },
